{
  "name": "fuggit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/fuggit/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/fuggit/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/fuggit/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/fuggit",
        "tsConfig": "packages/fuggit/tsconfig.lib.json",
        "packageJson": "packages/fuggit/package.json",
        "main": "packages/fuggit/src/index.ts",
        "assets": [
          "packages/fuggit/*.md"
        ]
      }
    }
  },
  "tags": []
}
